@{
	ViewData["Title"] = "Home Page";
}

@section Styles {
	<link rel="stylesheet" type="text/css" href="~/styles/feedback_styles.css">
	<link rel="stylesheet" type="text/css" href="~/styles/feedback_responsive.css">
	<style>
		.test_content_container {
			position: absolute;
			left: 0;
			bottom: 0;
			width: 100%;
			height: auto;
			padding: 5px;
			background: #46a6a6b6;
		}

		.test_name {
			font-size: 14px;
			font-weight: 700;
			color: #ffffff;
			text-transform: uppercase;
		}
	</style>
}

<!-- Home -->

<div class="home">
	<div class="home_background parallax-window" data-parallax="scroll" data-image-src="../images/sobrenos.jpg"></div>
	<div class="home_content">
		<div class="home_title">Feedback</div>
	</div>
</div>



<!-- Blog -->

<div class="blog">
	<div class="container">
		<div class="row">
			

			<!-- Blog Content -->

			<div class="col-lg-8">
				

				<div class="blog_post_container">

					<!-- Blog Post -->

					<div class="blog_post">
						<div class="blog_post_image">
							<img src="../images/coquetel.jpg">
						</div>

					</div>


				</div>
			</div>
		</div>
	</div>

	<div class="testimonials">
		<div class="test_border"></div>
		<div class="container">
			<div class="row">
				<div class="col text-center">
					<h2 class="section_title">Momentos Especiais que o Turismo me proporciou</h2>
				</div>
			</div>
			<div class="row">
				<div class="col">

					<!-- Testimonials Slider -->
					<div class="test_slider_container">
						<div class="owl-carousel owl-theme test_slider">

							<!-- Testimonial Item -->
							<div class="owl-item">
								<div class="test_item">
									<div class="test_image">
										<img src="~/images/chile.png" alt="https://unsplash.com/@@tschax">
									</div>
									<div class="test_content_container">
										<div class="test_name text-white">Valle Nevado, Chile</div>
									</div>
								</div>
							</div>

							<!-- Testimonial Item -->
							<div class="owl-item">
								<div class="test_item">
									<div class="test_image">
										<img src="~/images/ancoradouro.png" alt="https://unsplash.com/@@tschax">
									</div>
									<div class="test_content_container">
										<div class="test_name text-white">Abertura do 27º Encontro Ancoradouro</div>
									</div>
								</div>
							</div>

							<!-- Testimonial Item -->
							<div class="owl-item">
								<div class="test_item">
									<div class="test_image">
										<img src="~/images/inauguraçãocruzeiro.png" alt="https://unsplash.com/@@tschax">
									</div>
									<div class="test_content_container">
										<div class="test_name text-white"> Inauguração do gigante MSC SEAVIEW</div>
									</div>
								</div>
							</div>

							<!-- Testimonial Item -->
							<div class="owl-item">
								<div class="test_item">
									<div class="test_image">
										<img src="~/images/SEASHORE.png" alt="https://unsplash.com/@@tschax">
									</div>
									<div class="test_content_container">
										<div class="test_name text-white">Inauguração do Navio MSC SEASHORE</div>
									</div>
								</div>
							</div>

						</div>



						<!-- Testimonials Slider Nav - Prev -->
						<div class="test_slider_nav test_slider_prev">
							<svg version="1.1" id="Layer_6" xmlns="http://www.w3.org/2000/svg"
								xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="28px" height="33px"
								viewBox="0 0 28 33" enable-background="new 0 0 28 33" xml:space="preserve">
								<defs>
									<linearGradient id='test_grad_prev'>
										<stop offset='0%' stop-color='#fa9e1b' />
										<stop offset='100%' stop-color='#8d4fff' />
									</linearGradient>
								</defs>
								<path class="nav_path" fill="#F3F6F9" d="M19,0H9C4.029,0,0,4.029,0,9v15c0,4.971,4.029,9,9,9h10c4.97,0,9-4.029,9-9V9C28,4.029,23.97,0,19,0z
