                            <div class="footer_nav">
                                <ul class="footer_nav_list">
                                    <li class="main_nav_item"><a asp-action="Index" asp-controller="Home">home</a></li>
                                    <li class="main_nav_item"><a asp-action="Ingressos" asp-controller="Home">Ingressos</a></li>
                                    <li class="main_nav_item"><a asp-action="Cruzeiros" asp-controller="Home">Cruzeiros</a></li>
